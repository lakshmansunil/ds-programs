#include<stdio.h>
void linearsearch(int a[20],int n,int x)
{
    int i,flag=0;
    for(i=0;i<n;i++)
    {
        if(a[i]==x)
        {
            flag=1;
            break;
        }
    }
    if(flag)
    {
        printf("element found\n");
    }
    else
    {
        printf("element not found\n");
    }
}
void main()
{
    int n,i,ele,a[10];
    printf("enter the size\n");
    scanf("%d",&n);
    for(i=0;i<n;i++)
    {
        scanf("%d",&a[i]);
    }
    printf("enter element to serach\n");
    scanf("%d",&ele);
    linearsearch(a,n,ele);
    }


